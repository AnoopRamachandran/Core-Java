
public class Design {

	public static void main(String args[])
	{
		for(int i=1;i<6;i++)
		{
			System.out.print("\n");
			for(int j=1;j<=i;j++){
				
				System.out.print(j);
			
				
			}
		}
		
		for(int x=4;x>1;x--)
		{
			System.out.print("\n");
			for(int y=1;y<=x;y++){
				
				System.out.print(y);
			
				
			}
		}
	}
}
